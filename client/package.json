{
  "name": "frontend-test",
  "version": "1.0.0",
  "description": "Superformula frontend test.",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "storybook": "start-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elijahboston/frontend-test.git"
  },
  "author": "Elijah Boston",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elijahboston/frontend-test/issues"
  },
  "homepage": "https://github.com/elijahboston/frontend-test#readme",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@storybook/addon-knobs": "^5.2.3",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "styled-jsx": "^3.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@storybook/react": "^5.2.3",
    "@svgr/webpack": "^4.3.3",
    "acorn": "^7.1.0",
    "ava": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-svg": "^1.0.1",
    "browser-env": "^3.2.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^3.2.0",
    "require-hacker": "^3.0.1",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "xo": "^0.25.3"
  },
  "browserslist": "> 0.25%, not dead",
  "xo": {
    "env": "mocha",
    "prettier": true,
    "fix": true,
    "global": [
      "document",
      "expect"
    ],
    "extends": "xo-react",
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ]
  },
  "ava": {
    "verbose": true,
    "require": [
      "esm",
      "@babel/register",
      "./test/helpers/_ignore-files.js",
      "./test/helpers/_setup-browser-env.js"
    ]
  }
}
